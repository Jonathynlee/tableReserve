    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
=======
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
>>>>>>> 8cc9c521ce140609992a3304f89736ac5769d2e1
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../mime/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../mime/cli.js" "$@"
  ret=$?
fi
exit $ret
